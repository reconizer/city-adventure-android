<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <pl.reconizer.cityadventure.presentation.customviews.ScalableImageView
        android:id="@+id/rankingTitleFrame"
        android:layout_width="219dp"
        android:layout_height="63dp"
        tools:src="@drawable/ranking_title_frame" />

    <pl.reconizer.cityadventure.presentation.customviews.ScalableImageView
        android:id="@+id/rankingTitleText"
        android:layout_width="122dp"
        android:layout_height="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/rankingTitleFrame"
        app:layout_constraintEnd_toEndOf="@+id/rankingTitleFrame"
        app:layout_constraintStart_toStartOf="@+id/rankingTitleFrame"
        app:layout_constraintTop_toTopOf="@+id/rankingTitleFrame"
        tools:src="@drawable/ranking_title" />
</androidx.constraintlayout.widget.ConstraintLayout>